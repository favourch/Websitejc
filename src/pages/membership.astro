---
import MainLayout from "@layouts/MainLayout.astro";
import Hero from "@components/membership/Hero.astro";
import Overview from "@components/membership/Overview.astro";
import Benefits from "@components/membership/Benefits.astro";
import GettingStarted from "@components/membership/GettingStarted.astro";
import MembershipTiers from "@components/membership/MembershipTiers.astro";

const title = "Membership | Wholesalers Advantage";
const description =
  "Join Wholesalers Advantage and become a member of our growing community. Get access to exclusive benefits and opportunities.";
---

<MainLayout title={title} description={description} navbarBackgroundColor="transparent">
  <Hero />
  <Overview />
  <MembershipTiers />
  <Benefits />
  <GettingStarted />
</MainLayout>
